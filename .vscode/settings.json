{
  "deno.enable": true,
  "deno.lint": true,
  "deno.suggest.imports.autoDiscover": true,
  "prettier.enable": false,
  "eslint.enable": false,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "editor.formatOnPaste": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "always",
    "source.organizeImports": "always"
  },
  "editor.defaultFormatter": "denoland.vscode-deno",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#2968e0",
    "activityBar.background": "#2968e0",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#ef90b0",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#2968e0",
    "statusBar.background": "#1b52bb",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#2968e0",
    "statusBarItem.remoteBackground": "#1b52bb",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#1b52bb",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#1b52bb99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#1b52bb"
}
